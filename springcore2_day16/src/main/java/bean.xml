<?xml version="1.0" encoding="UTF-8"?>
<beans

  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
        
        <bean id="e" class="com.springcore2_day16.Employee">
        <property name="id" value="07"></property>
        <property name="name" value="DIkshit"></property>
        <property name="address" ref="add"></property>
        </bean>
        <bean id="add" class="com.springcore2_day16.Address">
        <property name="city" value="Patna"></property>
        <property name="state" value="Bihar"></property>
        <property name="country" value="India"></property>
        
        </bean>
        
        
</beans>